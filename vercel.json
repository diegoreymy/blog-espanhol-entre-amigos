{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/espanhol-entre-amigos/browser",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/api/ssr.mjs"
    }
  ]
}
