{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/espanhol-entre-amigos/browser"
      }
    },
    {
      "src": "dist/espanhol-entre-amigos/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "prerenderConfig": {
    "routesFile": "routes.txt"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/espanhol-entre-amigos/server.mjs"
    }
  ]
}